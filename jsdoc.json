{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.(js|jsx|ts|tsx)$",
      "excludePattern": "(node_modules|dist|./src/stories|.+\\.stories\\.tsx$)"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "readme": "./README.md",
      "template": "default"
    }
}